<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="1" id="br.embrapa.ecow" ios-CFBundleVersion="1" version="1.18.02" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>e-cow</name>
    <description>Tomada de decisões relativas à reprodução de bovinos de corte.</description>
    <author email="camilo.carromeu@embrapa.br" href="https://www.embrapa.br/equipe/-/empregado/354215/camilo-carromeu">
        Camilo Carromeu
    </author>
    <content src="index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
</widget>
